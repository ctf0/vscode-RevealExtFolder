{
    "name": "revealextfolder",
    "displayName": "Reveal Ext Folder",
    "description": "Quickly reveal ext folder/finder/explorer",
    "version": "0.0.1",
    "publisher": "ctf0",
    "repository": "https://github.com/ctf0/vscode-RevealExtFolder",
    "engines": {
        "vscode": "^1.68.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onStartupFinished"
    ],
    "main": "./out/extension",
    "contributes": {
        "configuration": {
            "type": "object",
            "title": "Reveal Ext Folder",
            "properties": {
                "RevealExtFolder.openAsProject": {
                    "type": "boolean",
                    "default": false,
                    "description": "open ext directoy in vscode as a project instead in system finder/explorer"
                }
            }
        },
        "commands": [
            {
                "command": "extension.revealExtFolder",
                "title": "Reveal in folder",
                "icon": {
                    "dark": "./icons/reveal.svg",
                    "light": "./icons/reveal_light.svg"
                }
            }
        ],
        "menus": {
            "editor/title": [
                {
                    "command": "extension.revealExtFolder",
                    "group": "navigation",
                    "when": "activeEditor == workbench.editor.extension"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./"
    },
    "devDependencies": {
        "@types/node": "^18.11.9",
        "@types/vscode": "^1.68.0",
        "typescript": "^4.8.4"
    },
    "dependencies": {
        "open-file-explorer": "^1.0.2"
    }
}
